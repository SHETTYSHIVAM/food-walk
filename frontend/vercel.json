{
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    },
    "installCommand": "pnpm install",
    "buildCommand": "pnpm build",
    "routes": [
      {
        "src": "/[^.]+",
        "dest": "/"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  